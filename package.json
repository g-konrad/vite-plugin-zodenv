{
  "name": "vite-plugin-zodenv",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@7.2.1",
  "description": "Validate your environment variables with zod and vite",
  "author": "Gustavo Konrad <g.konrad@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/g-konrad/vite-plugin-zodenv.git"
  },
  "bugs": "https://github.com/g-konrad/vite-plugin-zodenv/issues",
  "keywords": [
    "vite",
    "zod"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint src/ --fix",
    "format": "prettier --write \"**/*.{ts,md,json}\"",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vite": "^2",
    "zod": "^3"
  },
  "devDependencies": {
    "@antfu/ni": "^0.17.2",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "bumpp": "^8.2.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "esno": "^0.16.3",
    "pnpm": "^7.5.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.14",
    "vitest": "^0.18.0",
    "zod": "^3.17.3"
  }
}
